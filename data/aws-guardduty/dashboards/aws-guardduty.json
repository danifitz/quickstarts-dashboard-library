{
	"name": "AWS GuardDuty",
	"description": null,
	"pages": [
		{
			"name": "AWS Security",
			"description": null,
			"widgets": [
				{
					"visualization": {
						"id": "viz.markdown"
					},
					"layout": {
						"column": 1,
						"row": 1,
						"height": 3,
						"width": 12
					},
					"title": "",
					"rawConfiguration": {
						"text": "# AWS GuardDuty Findings Dashboard\n\n## Severity Levels\nEvents are classified with a severity level between 1.0 and 8.9.\n* 1.0 - 3.9 - _Low_\n* 4 - 6.9 - _Medium_\n* 7 - 8.9 - _High_\n\n0-1 and 9-10 are currently reserved for future use. [Reference](https://docs.aws.amazon.com/guardduty/latest/ug/guardduty_findings.html)"
					}
				},
				{
					"visualization": {
						"id": "viz.billboard"
					},
					"layout": {
						"column": 1,
						"row": 4,
						"height": 3,
						"width": 3
					},
					"title": "Security Events by Severity",
					"rawConfiguration": {
						"dataFormatters": [],
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) FACET cases(WHERE detail.severity >= 7 AND detail.severity < 8.9  as 'High', WHERE detail.severity >= 4 AND detail.severity < 6.9 as 'Medium', WHERE detail.severity > 1 AND detail.severity < 3.9 as 'Low') LIMIT MAX"
							}
						],
						"thresholds": [
							{
								"alertSeverity": "WARNING",
								"value": 0.9
							},
							{
								"alertSeverity": "CRITICAL",
								"value": 1.9
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.line"
					},
					"layout": {
						"column": 4,
						"row": 4,
						"height": 3,
						"width": 9
					},
					"title": "Security Events",
					"rawConfiguration": {
						"dataFormatters": [],
						"legend": {
							"enabled": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) as 'GuardDuty Findings' WHERE detail.service.serviceName = 'guardduty' TIMESERIES AUTO"
							}
						],
						"yAxisLeft": {
							"zero": true
						}
					}
				},
				{
					"visualization": {
						"id": "viz.table"
					},
					"layout": {
						"column": 1,
						"row": 7,
						"height": 3,
						"width": 12
					},
					"title": "Security Event Details",
					"rawConfiguration": {
						"dataFormatters": [],
						"facet": {
							"showOtherSeries": false
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT detail.severity, detail.region, detail.type, detail.description WHERE detail.service.serviceName = 'guardduty' LIMIT 100"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 1,
						"row": 10,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by Resource Type",
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) FACET detail.resource.resourceType "
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 5,
						"row": 10,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by Country",
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) FACET detail.service.action.awsApiCallAction.remoteIpDetails.country.countryName"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 9,
						"row": 10,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by ISP",
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) FACET detail.service.action.awsApiCallAction.remoteIpDetails.organization.asnOrg"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 1,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by Action Type",
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) WHERE detail.service.serviceName = 'guardduty' FACET detail.service.action.actionType"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 5,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by IP Address",
					"rawConfiguration": {
						"dataFormatters": [],
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) WHERE detail.service.serviceName = 'guardduty' FACET detail.service.action.awsApiCallAction.remoteIpDetails.ipAddressV4"
							}
						]
					}
				},
				{
					"visualization": {
						"id": "viz.pie"
					},
					"layout": {
						"column": 9,
						"row": 13,
						"height": 3,
						"width": 4
					},
					"title": "Security Events by AWS Region",
					"rawConfiguration": {
						"facet": {
							"showOtherSeries": true
						},
						"nrqlQueries": [
							{
								"accountId": 0,
								"query": "FROM Log SELECT count(*) FACET region WHERE detail.service.serviceName = 'guardduty'"
							}
						]
					}
				}
			]
		},
		{
			"name": "Security Events",
			"description": null,
			"widgets": [
